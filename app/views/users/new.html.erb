<h3>This page is to create user's account</h3>

<%= render 'shared/error_messages', object: @user %>

<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <br/>

    <%= f.submit 'Register' %>
<% end %>
